{
   "manifest_version": 3,
   "name": "canvas-rr (content script)",
   "version": "0.6",

   "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*", "file://*/*"],
        "js": ["out/rr-record.js"],
        "run_at": "document_start",
        "world": "MAIN",
        "all_frames": true
      }
   ]
}
