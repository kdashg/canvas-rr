{
   "manifest_version": 2,
   "name": "canvas-rr",
   "version": "0.5",

   "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*", "file://*/*"],
        "js": ["out/rr-record.js"],
        "run_at": "document_start",
        "world": "MAIN",
        "all_frames": true
      }
   ]
}
